@import '~assets/less/variable';

.scheduleEditor {
  min-height: 0;
  flex: 1;
  display: flex;
  flex-direction: column;

  .containerVertical {
    flex: 1;
    padding: 16px;
    overflow-y: auto;
  }
}

.info {
  color: @disabled-color;
  
  i {
    margin-left: 8px;
    font-size: 18px;
    color: @error-color;
    vertical-align: middle;
    cursor: pointer;
  }
}

.log {
  width: 900px;
}
